<div class="reversed">
    <div class="container text-center">
        {% if block.title|length %}
            <h2>{{ block.title }}</h2>
        {% endif %}

        <div class="row">
            <div class="col-12 col-md-10">
                <iframe width="100%" height="350" data-aos="fade-up" data-aos-duration="1000" src="{{ video.embed }}?rel=0" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
        </div>
    </div>
</div>