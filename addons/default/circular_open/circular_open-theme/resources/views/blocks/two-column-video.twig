<div class="container text-center">
    <h2>{{ block.title }}</h2>
    <div class="row">
        {% for vid in block.video_embed %}
            <div class="col-12 col-md-6">
                <iframe width="100%" height="350" src="{{ vid.video.embed }}?rel=0" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
        {% endfor %}
    </div>
</div>