<div class="container">
    <div class="row">
        <div class="col-12 col-md-4" data-aos="fade-right" data-aos-duration="1000">
            {% if block.title|length %}
                <h1>{{ block.title }}</h1>
            {% endif %}

            <a href={{block.cta_link}}>{{ block.cta_label }}</a>
            {{ block.content|raw }}
        </div>
        <div class="col-12 col-md-8 align-self-end" data-aos="fade-up" data-aos-duration="1000">
            <iframe src="{{ block.map|raw }}" width="100%" height="500px" style="border:none;"></iframe>
        </div>
    </div>
</div>
