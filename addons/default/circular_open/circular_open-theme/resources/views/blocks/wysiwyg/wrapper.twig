{# Overrides core content wrapper #}
<div class="container-fluid">
    <div id="block--{{ block.id }}" class="block block--{{ block.extension.value.slug }}">
        <div class="block__content">
            {% if block.title.value and not block.display_title.isFalse() %}
                <h2 class="block__title">
                    {{ block.title }}
                </h2>
            {% endif %}

            {{ content|raw }}
        </div>
    </div>
</div>